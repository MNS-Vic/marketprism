# Monitoring & Alerting module environment variables
# Copy to .env and fill in values for production use

# Grafana admin credentials (used by docker-compose)
GRAFANA_ADMIN_USER=admin
GRAFANA_ADMIN_PASSWORD=change_me

# DingTalk webhook integration (used by docker-compose)
# Obtain from DingTalk robot settings
DINGTALK_WEBHOOK_URL=
DINGTALK_SECRET=

# Optional middlewares toggles (used by main.py)
# Enable only when exposing to external network or integration needs
MARKETPRISM_ENABLE_AUTH=false
MARKETPRISM_ENABLE_VALIDATION=false

